{
    "configurations": [
        {
            "name": "Win32",
            "intelliSenseMode": "gcc-x86",
            "compilerPath": "C:/MinGW/bin/g++.exe",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/src/include",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}